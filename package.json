{
  "name": "node-typescript-boilerplate",
  "version": "0.0.0",
  "description": "Minimalistic boilerplate to quick-start Node.js development in TypeScript.",
  "engines": {
    "node": ">= 22.11 < 23"
  },
  "devDependencies": {
    "@eslint/js": "~9.39",
    "@types/eslint__js": "~9.14",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/parser": "~8.48",
    "@vitejs/plugin-react-swc": "^4.2.2",
    "@vitest/coverage-v8": "~4.0",
    "@vitest/eslint-plugin": "~1.5",
    "eslint": "~9.39",
    "eslint-config-prettier": "~10.1",
    "globals": "~16.5",
    "prettier": "~3.7",
    "rimraf": "~6.1",
    "ts-api-utils": "~2.1",
    "ts-node": "^10.9.2",
    "typescript": "~5.9",
    "typescript-eslint": "~8.48",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "~4.0"
  },
  "scripts": {
    "start": "node build/src/index.js",
    "clean": "rimraf coverage build tmp",
    "build": "tsc -p tsconfig.json",
    "test": "vitest",
    "prettier": "prettier \"{src,tests}/**/*.{ts,mts}\" --config .prettierrc --write",
    "prettier:check": "prettier \"{src,tests}/**/*.{ts,mts}\" --config .prettierrc --check",
    "test:watch": "vitest",
    "frontend": "vite frontend --config ./frontend/vite.config.ts"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@types/express": "^5.0.6",
    "express": "^5.2.1",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "tslib": "~2.8"
  },
  "volta": {
    "node": "22.12.0"
  }
}
